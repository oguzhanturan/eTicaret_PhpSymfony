
    <form action="{{ path('admin_image_new',{'pid': pid}) }}" method="post" enctype="multipart/form-data">
        Select image to upload:
        <input name="productid" value="{{ pid }}" type="hidden">
        <input type="file" name="name" id="fileToUpload">
        <input type="submit" value="Upload Image" name="submit">

    </form>
    {% for image in imagelist %}
    <img src="{{ asset('admin/uploads') }}/images/{{ image.image }}" height="100">
    {% endfor %}